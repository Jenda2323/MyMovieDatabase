@using MovieProject.DTO

@model MovieDTO
<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Title"> </label>
        <input type="text" asp-for="Title" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Director"> </label>
        <input type="text" asp-for="Director" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Genre"> </label>
        <input type="text" asp-for="Genre" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ReleaseDate"> </label>
        <input type="number" asp-for="ReleaseDate" class="form-control" />
    </div>
    <input type="submit" class="btn btn-success my-2" value="Create" />
</form>
